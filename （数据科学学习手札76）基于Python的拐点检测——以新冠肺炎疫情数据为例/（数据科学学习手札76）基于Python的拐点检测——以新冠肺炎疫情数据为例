  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def knee_point_search(x, y):\n",
    "    \n",
    "    # list\n",
    "    x, y = x.tolist(), y.tolist()\n",
    "    output_knees = []\n",
    "    for curve in ['convex', 'concave']:\n",
    "        for direction in ['increasing', 'decreasing']:\n",
    "            model = KneeLocator(x=x, y=y, curve=curve, direction=direction, online=False)\n",
    "            if model.knee != x[0] and model.knee != x[-1]:\n",
    "                output_knees.append((model.knee, model.knee_y, curve, direction))\n",
    "    \n",
    "    if output_knees.__len__() != 0:\n",
    "        print('')\n",
    "        return output_knees\n",
    "    else:\n",
    "        print('')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
